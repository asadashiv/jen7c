pipeline {
    agent none
      
    stages {
        stage('clone'){
        agent any
            steps {
                
                echo " cloned from git"
                sh 'sleep 3'
            
            }
        }
        stage('build'){
           agent {label 'slavec'}
            steps {
                sh '''
                cd /home/jenkins/workspace/buildc
                make 
                '''
            }
        }
    }
}
